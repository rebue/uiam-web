{
    "name": "uiam-web",
    "productName": "uiam-web",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": ".vite/build/main.js",
    "scripts": {
        "dev": "rimraf dist && vite -c vite.renderer.config.ts",
        "build": "rimraf dist && vue-tsc --noEmit && vite build -c vite.renderer.config.ts",
        "start": "electron-forge start",
        "package": "rimraf out && electron-forge package --platform win32 && cpy 'node_modules' out/uiam-web-win32-x64/resources/app && asar pack out/uiam-web-win32-x64/resources/app out/uiam-web-win32-x64/resources/app.asar && del out/uiam-web-win32-x64/resources/app",
        "make": "rimraf out && electron-forge make --platform win32 && cpy 'node_modules' out/uiam-web-win32-x64/resources/app && asar pack out/uiam-web-win32-x64/resources/app out/uiam-web-win32-x64/resources/app.asar && del out/uiam-web-win32-x64/resources/app",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx ."
    },
    "keywords": [],
    "author": {
        "name": "zbz",
        "email": "nnzbz@21cn.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@electron-forge/cli": "^7.2.0",
        "@electron-forge/maker-deb": "^7.2.0",
        "@electron-forge/maker-rpm": "^7.2.0",
        "@electron-forge/maker-squirrel": "^7.2.0",
        "@electron-forge/maker-zip": "^7.2.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
        "@electron-forge/plugin-vite": "^7.2.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "@vitejs/plugin-vue": "^5.0.3",
        "cpy-cli": "^5.0.0",
        "del-cli": "^5.1.0",
        "electron": "^28.1.4",
        "eslint": "^8.0.1",
        "eslint-plugin-import": "^2.25.0",
        "path": "^0.12.7",
        "rimraf": "^5.0.5",
        "ts-node": "^10.0.0",
        "typescript": "~4.5.4",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "electron-squirrel-startup": "^1.0.0",
        "vue": "^3.4.15"
    }
}
