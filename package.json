    "version": "1.0.0",
    "description": "统一身份认证和访问管理平台的前端",
    "main": ".vite/build/main.js",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist & rimraf out",
        "dev": "vite -c vite.renderer.config.ts",
        "prebuild": "npm run clean",
        "build": "vue-tsc --noEmit && vite build -c vite.renderer.config.ts",
        "start": "electron-forge start",
        "premake": "npm run clean",
        "make": "electron-forge make --platform win32",
        "prepublish": "npm run clean",
        "publish": "electron-forge publish --platform win32"
    },
    "keywords": [
        "electron",
        "vite",
        "vue",
        "IAM"
